{
    "name": "charlgottschalk/laravel-relix",
    "description": "Database seeder generator for MySQL, PostgreSQL, and SQLite",
    "version": "0.1.0-beta.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CharlGottschalk\\LaravelRelix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CharlGottschalk\\LaravelRelix\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Charl Gottschalk",
            "email": "2204385+CharlGottschalk@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.7|^4.0",
        "fakerphp/faker": "^1.23",
        "illuminate/console": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/view": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CharlGottschalk\\LaravelRelix\\LaravelRelixServiceProvider"
            ]
        }
    }
}
